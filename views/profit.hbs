<div class="hackathon-header">
  <div class="logo">
    <p class="logo-icon" data-brand="anf" data-type="horizontal" aria-hidden="true"></p>
    <span class="screen-reader-text">abercrombie and fitch</span>
  </div>
  <h1 class="screen-reader-text">{{title}}</h1>
</div>

<div class="data-card" data-variant="boxed" data-header>
  {{#if SHOW_MOTTOS}}
    <div class="motto-wrapper h1"><p class="motto">Aim High</p><p class="motto">Get Shit Done</p><p class="motto">Build Up</p><p class="motto">Team Up</p><p class="motto">Show Up</p><p class="motto">Stay Curious</p><p class="motto">Push Boundaries</p></div>
  {{/if}}

  <div class="data-card-header">
    <h2 class="h2">Request Context</h2>
  </div>
  <div class="data-card-content">
    <div>
      <p>SERVER_LOCATION: {{SERVER_LOCATION}}</p>
    </div>
  </div>
</div>

<div class="data-card" data-variant="boxed" data-header>
  <div class="data-card-header">
    <h2 class="h2">Daily Revenue</h2>
  </div>
  <div class="data-card-content">
    <div>
      <p>Total Profit: ${{profit}}</p>
      <p>Previous Profit: ${{previousProfit}}</p>
      <p>Orders: {{orders.length}}</p>
    </div>
  </div>
</div>

<div class="data-card" data-variant="boxed" data-header>
  <div class="data-card-header">
    <h3 class="h3">Orders</h3>
  </div>
  <div class="data-card-content">
    <div>
      {{#each orders}}
        <div class="order-card">
          <p>Order ID: #{{orderid}}</p>
          <p>Created on: {{createdOn}}</p>
        </div>
      {{/each}}
    </div>
  </div>
</div>
